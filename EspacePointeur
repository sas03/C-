#include <stdio.h>
#include <stdlib.h>
 
int* zahlen_1_bis_n(int n){
	int *zahlen = malloc(sizeof(int) * n);
	for(int i=0; i<n; i++){
		zahlen[i] = i+1;
		printf("%d",zahlen[i]);
	}
	return zahlen;
}
 
int main(void) {
	//int x = 127;
	//int d = ++x;
	unsigned char x = 127;
	unsigned char d = ++x;
	printf("%i und %i",x,d);
 
	return 0;
}
